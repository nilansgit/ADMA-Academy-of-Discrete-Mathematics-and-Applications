datasource db {
  provider = "mysql"
}

model users {
  id         Int         @id @default(autoincrement())
  email      String      @unique(map: "email") @db.VarChar(255)
  password   String      @db.VarChar(255)
  role       users_role?
  created_at DateTime?   @default(now()) @db.Timestamp(0)
}

enum users_role {
  admin
  treasurer
  secretary
}
