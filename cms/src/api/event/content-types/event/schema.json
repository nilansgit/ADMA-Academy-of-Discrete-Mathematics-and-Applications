{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Event"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true
    },
    "Description": {
      "type": "string"
    },
    "Date": {
      "type": "date"
    },
    "Event_Status": {
      "type": "enumeration",
      "required": true,
      "default": "Upcoming",
      "enum": [
        "Upcoming",
        "Past",
        "Archived"
      ]
    },
    "PDF": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "files"
      ]
    },
    "Link": {
      "type": "string"
    }
  }
}
